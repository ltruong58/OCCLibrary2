<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.orangecoastcollege.cs273.nhoang53.occlibrary2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Welcome to OCC Library">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/sign_in"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentProfileActivity"
            android:label="@string/student_profile" />
        <activity android:name=".PickDateActivity" />
        <activity android:name=".PickRoomActivity" />
        <activity android:name=".PickBookingTimeActivity"></activity>
    </application>

</manifest>